#include <stdio.h>
#include <cs50.h>
#include <string.h>

void menu(void);

int main(void)
{
    menu();
}

void menu(void)
{
    

    string day = get_string("오늘은 무슨 요일입니까?:\n");
   
    if(strcmp(day,"월요일")==0)
    {
        printf("청국장\n");
    }
    else if(strcmp(day,"화요일")==0)
    {
        printf("비빔밥\n");
    }
    else
    {
        printf("오삼 불고기\n");
    }
}
